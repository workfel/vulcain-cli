{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch",
			"type": "node",
			"request": "launch",
			"preLaunchTask": "compile-ts",
			"program": "${workspaceRoot}/src/index.ts",
			"stopOnEntry": false,
			"args": ["project", "test", "test-oauth", "-t", "NodeEmptyApplication"],
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--harmony"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"externalConsole": true,
			"sourceMaps": true,
			"outDir": "${workspaceRoot}/dist",
			"smartStep": true
		},
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5858
		}
	]
}